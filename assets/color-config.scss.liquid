.btn-button.readmore, .spr-button.readmore,
.btn-button.bordered, .spr-button.bordered {
  color: $HeadingColor;
}
//************* Pagination *************/
.main-container .pagenav-wrap .pagination-search .pagination-page li.active span, .main-container .pagenav-wrap .pagination .pagination-page li.active span {
  background: $HeadingColor;
}
//************* Blog *************/
{% if settings.blog_page_layout == 'style_2' or settings.blog_page_layout == 'style_4' %}
.template-blog {
  .main-container {
    background: #F7F7F7;
  }
}
{% endif %}
//************* Shortcode *************/
.incify--block-title.style_1 .under--line {
  border-color: $HeadingColor;
}
.service-quote-box p,
.progress-title
{
  color: $HeadingColor;
}
.incify-member__inner.style_1 .member-social.item--social a {
  color: $BasicTextColor;
  &:hover {
    color: $ThemeColor;
  }
}
.incify-member__inner.style_2 .member-social.item--social a {
  color: $HeadingColor;
  &:hover {
    color: $ThemeColor;
  }
}
//************* Shop *************/
.main-container .sidebar .block .narrow-by-list dd ol.swatch-list li .item span.name {
  &:hover {
    color: $HeadingColor;
    border-color: $HeadingColor;
  }

}
.organic-deal-box .shortcode-countdown .text {
  color: $HeadingColor;
  font-family: $fontFamily, arial, serif ;
}
.wine-deal .shortcode-countdown .product-date > div:before {
  border-color: $ThemeColor;
}
@media (max-width: 991px) {
  .main-section-header.header-transparent-enable.header--preset_dark .header-container {
      background: $HeadingColor !important;
  }
}
