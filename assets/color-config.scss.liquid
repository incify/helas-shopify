body {
  color:#777;
  font-size: $fontSize;
  font-family: $fontFamily, arial, serif ;
  h1 {
    font-size: $h1;
    font-family: $headingFontFamily;
    color: $HeadingColor;
  }
  h2 {
    font-size: $h2;
    font-family: $headingFontFamily;
    color: $HeadingColor;
  }
  h3 {
    font-size: $h3;
    font-family: $headingFontFamily;
    color: $HeadingColor;
  }
  h4 {
    font-size: $h4;
    font-family: $headingFontFamily;
    color: $HeadingColor;
  }
  h5 {
    font-size: $h5;
    font-family: $headingFontFamily;
    color: $HeadingColor;
  }
  h6 {
    font-size: $h6;
    font-family: $headingFontFamily;
    color: $HeadingColor;
  }
  #shopify-section-footer .footer-wrapper.footer-style5 .footer-container .content-text,
  .section__wine-deal .wine-deal .wine-deal__left-column .wine-deal__left-column--content
  {
    font-family: $headingFontFamily;
  }
  &.boxed{
    background-color:$OutOfBoxBackgroundColorForBoxedVersion!important;
    {% if settings.checkbox_background_image == true %}
    background-image:$OutOfBoxBackgroundImageForBoxedVersion!important;
    {% endif %}
  }
}
